<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
<mapper namespace="mapper.NowShowingMapper">
	
	<resultMap type="nowshowing" id="nowshowingMap">
		<result column="location_seq" property="locationSeq"/>
		<result column="location_name" property="locationName"/>
		<result column="branch_seq" property="branchSeq"/>
		<result column="branch_name" property="branchName"/>
		<result column="screening_seq" property="screeningSeq"/>
		<result column="screening_name" property="screeningName"/>
		<result column="film_seq" property="filmSeq"/>
		<result column="film_name" property="filmName"/>
		<result column="theater_seq" property="theaterSeq"/>
		<result column="theater_name" property="theaterName"/>
		<result column="theater_total_seat" property="theaterTotalSeat"/>
		<result column="theater_sold_seat" property="theaterSoldSeat"/>
		<result column="show_seq" property="showSeq"/>
		<result column="show_time" property="showTime"/>
	</resultMap>
	
	<select id="selectLocation" resultMap="nowshowingMap">
		select *
		from tb_now_showing
	</select>
</mapper>